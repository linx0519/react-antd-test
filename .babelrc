{
  "presets": [
    "@babel/react",
    "@babel/env",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      },
      "transform-runtime"
    ]
  ],
}
